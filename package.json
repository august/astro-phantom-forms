{
  "name": "astro-phantom-forms",
  "version": "0.1.0",
  "description": "Hide your forms from bots. Hydrate for humans. Astro's stealthiest anti-spam weapon.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components/PhantomForm.astro": "./dist/components/PhantomForm.astro"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "astro",
    "forms",
    "bot prevention",
    "hydration",
    "spam protection",
    "data island",
    "cloakform"
  ],
  "author": "August Trometer",
  "license": "MIT",
  "peerDependencies": {
    "astro": "^5.8.1"
  },
  "scripts": {
    "build": "rm -rf dist && mkdir -p dist/components && tsc && cp src/components/*.astro dist/components/",
    "dev": "tsc --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build && npm test"
  },
  "devDependencies": {
    "astro": "^5.8.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  }
}
